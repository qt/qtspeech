[access]
	inheritFrom = All-Non-CI-Projects
[access "refs/*"]
	owner = group Administrators
	create = group Registered Users
[project]
	description = The Qt Speech repository ssh://codereview.qt-project.org:29418/qt/qtspeech.git
[receive]
	requireContributorAgreement = true
	requireChangeId = true
[cherryPick]
	includeReviewedOn = true
[submit]
	action = cherry pick
	mergeContent = true
[cherryPick "categoryFooters"]
	Sanity-Review = true
